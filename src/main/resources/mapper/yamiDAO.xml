<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.yami.wechat.demo.dao.YamiDAO">
    <select id= "queryMyInfo" resultType="cn.yami.wechat.demo.dto.Yami">
        select name,email,qq,blog,id,message
        from boot_web.yami
        order by id desc
        limit 0,1
    </select>

</mapper>